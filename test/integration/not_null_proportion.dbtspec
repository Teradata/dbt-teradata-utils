target:
  type: teradata
  server: "{{ env_var('DBT_TERADATA_SERVER_NAME', 'localhost') }}"
  username: "{{ env_var('DBT_TERADATA_USERNAME', 'dbc') }}"
  password: "{{ env_var('DBT_TERADATA_PASSWORD', 'dbc') }}"
  schema: "dbt_test_{{ var('_dbt_random_suffix') }}"
  tmode: ANSI
  log: "0"

projects:
  - name: project_for_check_passes
    paths:
      dbt_project.yml: |
        name: 'project_for_check_passes'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: attrA
                tests:
                  - dbt_utils.not_null_proportion:
                      at_least: 0.5

      seeds/test_table1.csv: |
        id,attrA,attrB
        1,val1A,val1B
        2,,val2B
        2,val2A,val2B
        3,,val3B
        3,val3A,val3B
        3,val3A,val3B
        4,,val4B
        4,,val4B
        4,,val4B
        4,val4A,val4B

  - name: project_for_check_fails
    paths:
      dbt_project.yml: |
        name: 'project_for_check_passes'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: attrA
                tests:
                  - dbt_utils.not_null_proportion:
                      at_least: 0.6

      seeds/test_table1.csv: |
        id,attrA,attrB
        1,val1A,val1B
        2,,val2B
        2,val2A,val2B
        3,,val3B
        3,val3A,val3B
        3,val3A,val3B
        4,,val4B
        4,,val4B
        4,,val4B
        4,val4A,val4B

sequences:

  test_check_passes:
    project: project_for_check_passes
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_not_null_proportion_test_table1_0_5__attrA
        attributes:
          dbt_utils_not_null_proportion_test_table1_0_5__attrA.status: pass

  test_check_fails:
    project: project_for_check_fails
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_not_null_proportion_test_table1_0_6__attrA
        attributes:
          dbt_utils_not_null_proportion_test_table1_0_6__attrA.status: fail
