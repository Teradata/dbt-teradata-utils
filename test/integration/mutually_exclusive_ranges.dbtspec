sequences:
  test_check_passes:
    project: project_for_check_passes
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_mutually_exclusive_ranges_test_table1_not_allowed__min_age__max_age
        attributes:
          dbt_utils_mutually_exclusive_ranges_test_table1_not_allowed__min_age__max_age.status: pass

  test_check_fails:
    project: project_for_check_fails
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_mutually_exclusive_ranges_test_table1_not_allowed__min_age__max_age
