target:
  type: teradata
  server: "{{ env_var('DBT_TERADATA_SERVER_NAME', 'localhost') }}"
  username: "{{ env_var('DBT_TERADATA_USERNAME', 'dbc') }}"
  password: "{{ env_var('DBT_TERADATA_PASSWORD', 'dbc') }}"
  schema: "dbt_test_{{ var('_dbt_random_suffix') }}"
  tmode: ANSI
  log: "0"

projects:
  - name: project_for_check_passes
    paths:
      dbt_project.yml: |
        name: 'project_for_check_passes'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: id
                tests:
                  - dbt_utils.sequential_values

      seeds/test_table1.csv: |
        id
        1
        2
        3

  - name: project_for_check_fails
    paths:
      dbt_project.yml: |
        name: 'project_for_check_fails'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: id
                tests:
                  - dbt_utils.sequential_values

      seeds/test_table1.csv: |
        id
        1
        2
        4

  - name: project_for_check_passes_interval
    paths:
      dbt_project.yml: |
        name: 'project_for_check_passes_interval'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: id
                tests:
                  - dbt_utils.sequential_values:
                      interval: 2

      seeds/test_table1.csv: |
        id
        1
        3
        5

  - name: project_for_check_fails_interval
    paths:
      dbt_project.yml: |
        name: 'project_for_check_fails_interval'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: id
                tests:
                  - dbt_utils.sequential_values:
                      interval: 2

      seeds/test_table1.csv: |
        id
        1
        3
        4

  - name: project_for_check_passes_datepart
    paths:
      dbt_project.yml: |
        name: 'project_for_check_passes_datepart'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: heartbeat
                tests:
                  - dbt_utils.sequential_values:
                      datepart: day

      seeds/test_table1.csv: |
        id,heartbeat
        1,2022-01-01 13:04:34
        3,2022-01-02 13:04:34
        5,2022-01-03 13:04:34

  - name: project_for_check_fails_datepart
    paths:
      dbt_project.yml: |
        name: 'project_for_check_fails_datepart'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils

      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            columns:
              - name: heartbeat
                tests:
                  - dbt_utils.sequential_values:
                      datepart: day

      seeds/test_table1.csv: |
        id,heartbeat
        1,2022-01-01 13:04:34
        3,2022-01-02 13:04:34
        5,2022-01-04 13:04:34

sequences:
  test_check_passes:
    project: project_for_check_passes
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_id
        attributes:
          dbt_utils_sequential_values_test_table1_id.status: pass

  test_check_fails:
    project: project_for_check_fails
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_id
        attributes:
          dbt_utils_sequential_values_test_table1_id.status: fail

  test_check_passes_interval:
    project: project_for_check_passes_interval
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_id__2
        attributes:
          dbt_utils_sequential_values_test_table1_id__2.status: pass

  test_check_fails_interval:
    project: project_for_check_fails_interval
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_id__2
        attributes:
          dbt_utils_sequential_values_test_table1_id__2.status: fail

  test_check_passes_datepart:
    project: project_for_check_passes_datepart
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_heartbeat__day
        attributes:
          dbt_utils_sequential_values_test_table1_heartbeat__day.status: pass

  test_check_fails_datepart:
    project: project_for_check_fails_datepart
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_sequential_values_test_table1_heartbeat__day
        attributes:
          dbt_utils_sequential_values_test_table1_heartbeat__day.status: fail
