target:
  type: teradata
  server: "{{ env_var('DBT_TERADATA_SERVER_NAME', 'localhost') }}"
  username: "{{ env_var('DBT_TERADATA_USERNAME', 'dbc') }}"
  password: "{{ env_var('DBT_TERADATA_PASSWORD', 'dbc') }}"
  schema: "dbt_test_{{ var('_dbt_random_suffix') }}"
  tmode: ANSI
  log: "0"

projects:
  - name: equality_passes
    paths:

      dbt_project.yml: |
        name: 'equality_passes'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils
      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            tests:
              - dbt_utils.equality:
                  compare_model: ref('test_table2')

      seeds/test_table1.csv: |
        id,attrA,attrB
        1,val1A,val1B
        2,val2A,val2B
        3,val3A,val3B
        4,val4A,val4B

      seeds/test_table2.csv: |
        id,attrA,attrB
        1,val1A,val1B
        2,val2A,val2B
        3,val3A,val3B
        4,val4A,val4B

  - name: equality_fails
    paths:

      dbt_project.yml: |
        name: 'equality_fails'
        version: '1.0.0'
        config-version: 2
        dispatch:
          - macro_namespace: dbt_utils
            search_order:
              - teradata_utils
              - dbt_utils
      packages.yml: |
        packages:
        - package: dbt-labs/dbt_utils
          version: 0.8.0
        - local: "{{ env_var('DBT_TERADATA_UTILS_ROOT') }}"

      models/schema.yml: |
        version: 2
        models:
          - name: test_table1
            tests:
              - dbt_utils.equality:
                  compare_model: ref('test_table2')

      seeds/test_table1.csv: |
        id,attrA,attrB
        1,val1A,val1B
        2,val2A,val2B
        3,val3A,val3B
        4,val4A,val4B

      seeds/test_table2.csv: |
        id,attrA,attrB
        5,val1A,val1B
        6,val2A,val2B
        7,val3A,val3B
        8,val4A,val4B

sequences:
  test_equality_passes:
    project: equality_passes
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_equality_test_table1_ref_test_table2_
        attributes:
          dbt_utils_equality_test_table1_ref_test_table2_.status: pass

  test_equality_fails:
    project: equality_fails
    sequence:
      - type: dbt
        cmd: deps
      - type: dbt
        cmd: seed
      - type: run_results
        exists: True
      - type: dbt
        cmd: test
        check: false
      - type: run_results
        exists: True
        length: 1
        names:
          - dbt_utils_equality_test_table1_ref_test_table2_
        attributes:
          dbt_utils_equality_test_table1_ref_test_table2_.status: fail
